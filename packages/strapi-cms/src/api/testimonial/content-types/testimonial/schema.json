{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "Testimonial",
    "description": "Client testimonials and reviews"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string"
    },
    "content": {
      "type": "text",
      "required": true
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5,
      "default": 5
    },
    "image": {
      "type": "string"
    },
    "site": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::site.site",
      "inversedBy": "testimonials"
    }
  }
}
