    },
    "industry": {
      "type": "string",
      "required": true
    },
    "projectType": {
      "type": "string"
    },
    "challenge": {
      "type": "text"
    },
    "solution": {
      "type": "text"
    },
    "results": {
      "type": "json"
    },
    "testimonial": {
      "type": "text"
    },
    "clientRole": {
      "type": "string"
    },
    "beforeImage": {
      "type": "string"
    },
    "afterImage": {
      "type": "string"
    },
    "videoUrl": {
      "type": "string"
    },
    "metrics": {
      "type": "json"
    },
    "site": {
      "type": "relation",
